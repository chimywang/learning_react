{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": [1, 120],
    "no-shadow": 0,
    "no-confusing-arrow": 0,
    "no-console": 0,
    "arrow-body-style": 0,
    "no-unused-expressions": 0,
    "consistent-return": 0
  },
  "env": {
    "mocha": true
  }
}
